<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="heicub_world">
    <!-- Configure world with sun and ground plane. -->
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Add HeiCub to the world. -->
    <include>
      <uri>model://iCubHeidelberg01_no_weights</uri>
      <plugin name="AICubPlugin" filename="libgazeboAICubPlugin.so" />
    </include>
  </world>
</sdf>
